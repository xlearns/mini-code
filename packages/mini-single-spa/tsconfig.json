{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["src", "typings"],
	"exclude": ["node_modules", "**/dist", "**/__tests__/**/*"],
	"compilerOptions": {
		"allowJs": true,
		"module": "ES6",
		"target": "ES2018",
		"noImplicitAny": false,
		"declaration": true,
		"sourceMap": true,
		"lib": ["ES2018", "DOM", "DOM.Iterable"],
		"allowSyntheticDefaultImports": true,
		"types": []
	}
}
